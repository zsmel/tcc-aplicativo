<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reconhecimentofacial">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="S.O.S People"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReconhecimentoFacial">
        <activity
            android:name=".FormLogin"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FormCadastroAdm"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FormCadastro"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FormLog"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".Monitoramento"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CaseDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".EditCaseActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".FormCadastroDesaparecido"
            android:screenOrientation="portrait"
            android:exported="false" />


        <activity
            android:name=".FormCadastroComunicante"
            android:screenOrientation="portrait"
            android:exported="false" />



    </application>

</manifest>